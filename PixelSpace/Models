CREWS 

import Foundation


enum MemberCrews: String, CaseIterable, Identifiable {
    case crew1
    case crew2
    case crew3
    case crew4

    var id: Self { self }
    
    struct MemberCrewInfo: Identifiable {
        var id = UUID()
        var name: String
        var role: String
        var background: String
        var birthdate: String
        var description: String
        var countryFlag: String
        var country: String
    }

    var info: MemberCrewInfo {
        switch self {
        case .crew1:
            return MemberCrewInfo(
                name: "Frank Borman",
                role: "Commandant",
                background: "Astronaute de la NASA",
                birthdate: "14 mars 1928",
                description: "Frank Borman est le commandant de la mission Apollo 8...",
                countryFlag: "ðŸ‡ºðŸ‡¸",
                country: "Etats-unis"
            
            )

        case .crew2:
            return MemberCrewInfo(
                name: "James A. Lovell Jr.",
                role: "Pilot du module de commande",
                background: "Astronaute de la NASA",
                birthdate: "18 avril 1932",
                description: "James A. Lovell Jr. est un astronaute amÃ©ricain connu pour...",
                countryFlag: "ðŸ‡¯ðŸ‡µ",
                country: "Japon"
            )

        case .crew3:
            return MemberCrewInfo(
                name: "William A. Anders",
                role: "Pilote du module lunaire",
                background: "Astronaute de la NASA",
                birthdate: "17 octobre 1933",
                description: "William Anders est cÃ©lÃ¨bre pour la photo Earthrise...",
                countryFlag: "ðŸ‡ºðŸ‡¸",
                country: "Etats-unis"
            )

        case .crew4:
            return MemberCrewInfo(
                name: "Julie Huhue",
                role: "Scientist",
                background: "Astronaute de la NASA",
                birthdate: "â€”",
                description: "Scientifique spÃ©cialisÃ©e dans les missions spatiales...",
                countryFlag: "ðŸ‡§ðŸ‡·",
                country: "BrÃ©zil"
            )
        }
    }
}



