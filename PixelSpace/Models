import Foundation

enum Fusees: String, CaseIterable, Identifiable {
    case apollo1
    case apollo7
    case apollo8
    case apollo9
    case STS1
    case crew1
    case soyouz1
    case crew2
    
    var id: Self { self }
    
    var name: String {
        switch self {
        case .apollo1:
            "Apollo 01"
        case .apollo7:
            "Apollo 07"
        case .apollo8:
            "Apollo 08"
        case .apollo9:
            "Apollo 09"
        case .STS1:
            "STS-1"
        case .crew1:
            "Crew-1"
        case .soyouz1:
            "Soyouz 01"
        case .crew2:
            "Crew-2"
        }
    }
    
    var image: String {
        switch self {
        case .apollo1: "apollo"
        case .apollo7: "apollo"
        case .apollo8: "apollo"
        case .apollo9: "apollo"
        case .STS1: "navette"
        case .crew1: "crew"
        case .soyouz1: "soyouz"
        case .crew2: "crew"
        }
    }
    
    var description: String {
        switch self {
        case .apollo1:
            ""
        case .apollo7:
            ""
        case .apollo8:
            "Apollo 8 fut la premiÃ¨re mission habitÃ©e Ã  quitter lâ€™orbite terrestre et Ã  orbiter autour de la Lune. LancÃ©e le 21 dÃ©cembre 1968, elle transporta Frank Borman, James Lovell et William Anders Ã  bord du module de commande Columbia. AprÃ¨s un voyage de prÃ¨s de 400 000 km, lâ€™Ã©quipage effectua 10 orbites lunaires en 20 heures, rÃ©alisant des observations dÃ©taillÃ©es et capturant la cÃ©lÃ¨bre photo â€œEarthriseâ€, montrant la Terre se levant au-dessus de lâ€™horizon lunaire. Le 24 dÃ©cembre, ils diffusÃ¨rent en direct une lecture de la GenÃ¨se, suivie par des millions de personnes sur Terre. La mission sâ€™acheva avec un retour rÃ©ussi le 27 dÃ©cembre 1968, prÃ©parant ainsi le terrain pour lâ€™alunissage dâ€™Apollo 11 lâ€™annÃ©e suivante."
        case .apollo9:
            ""
        case .STS1:
            ""
        case .crew1:
            ""
        case .soyouz1:
            ""
        case .crew2:
            ""
        }
    }
    
    var crewMemberParticipant: [MemberCrews] {
        switch self {
        case .apollo1:
            [.crew1, .crew2]
        case .apollo7:
            [.crew3, .crew4]
        case .apollo8:
            [.crew1, .crew2, .crew3]
        case .apollo9:
            [.crew3, .crew4]
        case .STS1:
            [.crew3, .crew4]
        case .crew1:
            [.crew3, .crew4]
        case .soyouz1:
            [.crew3, .crew4]
        case .crew2:
            [.crew3, .crew4]
        }
    }
    
}

